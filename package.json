      "eslint-config-airbnb-base": "^15.0.0",
      "eslint-plugin-import": "^2.28.1",
      "eslint-plugin-prettier": "^4.0.0",
      "husky": "^8.0.1",
      "jest": "^27.5.1",
      "size-limit": "^7.0.8",
      "ts-jest": "^27.1.5",
      "tsdx": "^0.14.1",
