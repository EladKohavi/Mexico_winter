      }
    },
    "name": "@linearb/gitstream-core",
    "author": "Oriel Zaken",
    "module": "dist/gs-rule-parser.esm.js",
    "size-limit": [
      {
        "path": "dist/gs-rule-parser.cjs.production.min.js",
